        "./src/containers/Cart.tsx",
        "./src/containers/Main.tsx",
        "./src/containers/Navbar.tsx",
        "./src/containers/Product.tsx",
        "./src/containers/ProductList.tsx",
        "./src/actions/index.ts",
        "./src/actions/productPage/index.ts",
        "./src/api/shop.ts",
        "./src/configureReducers.ts",
        "./src/constants/ActionTypes.ts",
        "./src/constants/productPage/ActionTypes.ts",
        "./src/interfaces/AppPage.ts",
        "./src/interfaces/Product.ts",
